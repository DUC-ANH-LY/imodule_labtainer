web-mysql = client:.bash_history : CONTAINS : sudo mysql -u root < ./src/users_account.sql
web-php = client:.bash_history : CONTAINS : sudo cp ./src/html/* /var/www/html/
modsec-config = client:.bash_history : CONTAINS : sudo tail -f /var/log/apache2/modsec_audit.log
filebeat-path = client:cat.stdout : CONTAINS : /var/log/apache2/modsec_audit.log
filebeat-IP = client:cat.stdout : CONTAINS : 172.0.0.3:5044
logstash = server:cat.stdout : CONTAINS : elastic
elasticsearch = server:cat.stdout : CONTAINS : xpack.security.enabled: true
kibana = server:cat.stdout : CONTAINS : xpack.security.enabled: true
attack-sqli = attacker:.bash_history : CONTAINS : firefox &
modsec-logs = server:cat.stdout : CONTAINS : +OR+1%3D1+
modsec-alert = server:cat.stdout : CONTAINS : ' OR 1=1 --
